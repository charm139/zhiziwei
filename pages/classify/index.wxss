.box_main{
    width: 100%;
    height: auto;
    overflow: hidden;
}
.box_content{
  width: 100%;
  height: auto;
  overflow: hidden;
  column-count: 3;
  -webkit-column-count: 3;
  -webkit-column-gap: 9px;
  column-gap: 9px;
}
.box_item{
  float: left;
  width:230rpx ;
  height: auto;
  display: inline-block;
  margin-top:1%;
  overflow: hidden;
}